<div class="block">
<div class="content">          
  <h2 class="title"></h2>
  <div class="inner">
	
		<div id="diary">
			<ul id="content">
				<% ('A'..'Z').each do |alphabet| %>
					<li><a href="#"><%= alphabet.to_s %></li>
				<% end %>
			</ul>
			<div class="diary_item">
				<table class="table">
		      <tr>             
		        <th>Descrição</th>
						<th>Info</th>
		        <th class="last">&nbsp;</th>
		      </tr>
		      <% @user.contacts.each do |contact| -%>
		      <tr class="<%= cycle("odd", "even") %>">
						<td><%= contact.description %></td>
						<td><%= link_to contact.info, [@user, contact] %></td>
		        <td class="last">
		          <%= link_to "#{t("web-app-theme.show", :default => "Show")}", [@user, contact] %> |
		          <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_user_contact_path(@user, contact) %> |
		          <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", [@user, contact], :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
		        </td>
		      </tr>          
		      <% end -%>
		    </table>
			</div>
		</div>

    <div class="actions-bar wat-cf">
      <div class="actions">
      </div>
    </div>
  </div>
</div>
</div>
<% content_for :sidebar, render(:partial => 'sidebar') -%>
